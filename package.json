{
  "name": "livepool",
  "version": "0.7.2",
  "description": "debugging proxy for web developers base on NodeJS",
  "keywords": [
    "proxy",
    "debug",
    "web",
    "develop",
    "reload",
    "fiddler"
  ],
  "maintainers": [
    {
      "name": "rehornchen@tencent.com",
      "web": "https://github.com/rehorn"
    }
  ],
  "homepage": "https://github.com/rehorn/livepool",
  "dependencies": {
    "colors": "*",
    "express.io": "*",
    "graceful-fs": "*",
    "http-proxy": "*",
    "http-proxy-agent": "*",
    "iconv-lite": "*",
    "mime": "*",
    "request": "*",
    "rimraf": "*",
    "underscore": "*"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rehorn/livepool.git"
  },
  "engines": {
    "node": ">= 0.8.x"
  },
  "bugs": {
    "url": "https://github.com/rehorn/livepool/issues"
  },
  "bin": {
    "livepool": "./bin/livepool.js"
  },
  "preferGlobal": true,
  "main": "node-webkit.html",
  "webkit": {
    "plugin": true
  },
  "window": {
    "title": "livepool ui",
    "toolbar": false,
    "frame": true,
    "width": 960,
    "height": 600,
    "min_width": 400,
    "min_height": 200
  },
  "node-main": "./node-webkit.js"
}
